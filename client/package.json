{
    "name": "goals-marketplace-next-app",
    "description": "Goals Web Application - Client",
    "version": "0.1.0",
    "private": true,
    "author": "goals",
    "contributors": [
        "Nishil Faldu <faldund@outlook.com>"
    ],
    "engines": {
        "node": ">=16.0.0",
        "yarn": ">=1.22.0",
        "npm": "please-use-yarn"
    },
    "license": "UNLICENSED",
    "repository": "",
    "scripts": {
        "dev": "yarn generate && next dev",
        "compile": "yarn generate && tsc -p . --pretty",
        "build": "yarn generate && next build",
        "export": "yarn generate && next build && next export",
        "start": "next start",
        "lint": "next lint --dir .",
        "test": "yarn jest",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "generate": "echo 'run-p generate:graphql'",
        "prettier": "prettier --write .",
        "dev-debug": "cross-env NODE_OPTIONS='--inspect' next dev",
        "generate:graphql": "graphql-codegen --config codegen.yml",
        "generate:paths": "ts-node --skipProject --transpile-only scripts/generatePaths.ts",
        "generate:selectors": "ts-node --skipProject --transpile-only scripts/generateSelectors.ts",
        "generate:version": "ts-node --skipProject --transpile-only scripts/generateVersion.ts"
    },
    "dependencies": {
        "@apollo/client": "^3.8.1",
        "@heroicons/react": "^2.0.18",
        "@reduxjs/toolkit": "^1.9.5",
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "antd": "^5.8.5",
        "autoprefixer": "^10.4.15",
        "clsx": "^2.0.0",
        "graphql": "^16.8.0",
        "lodash": "^4.17.21",
        "next": "^13.4.19",
        "postcss": "^8.4.29",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.4",
        "react-icons": "^4.10.1",
        "react-redux": "^8.1.2",
        "sass": "^1.66.1",
        "tailwindcss": "^3.3.3",
        "yarn-run-all": "^3.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.11",
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/typed-document-node": "^5.0.1",
        "@graphql-codegen/typescript": "^4.0.1",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@storybook/addon-actions": "^7.4.0",
        "@storybook/addon-essentials": "^7.4.0",
        "@storybook/addon-interactions": "^7.4.0",
        "@storybook/addon-links": "^7.4.0",
        "@storybook/addon-mdx-gfm": "^7.4.0",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/mdx2-csf": "^1.1.0",
        "@storybook/nextjs": "^7.4.0",
        "@storybook/react": "^7.4.0",
        "@storybook/testing-library": "^0.2.0",
        "@testing-library/jest-dom": "^6.1.2",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.5.7",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@types/react-test-renderer": "^18.0.0",
        "@types/uuid": "^9.0.3",
        "babel-loader": "^9.1.3",
        "eslint": "^8.48.0",
        "eslint-config-next": "^13.4.19",
        "eslint-plugin-storybook": "^0.6.13",
        "jest": "^29.6.4",
        "jest-environment-jsdom": "^29.6.4",
        "react-test-renderer": "^18.2.0",
        "storybook": "^7.4.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "typescript-eslint": "^0.0.1-alpha.0"
    }
}